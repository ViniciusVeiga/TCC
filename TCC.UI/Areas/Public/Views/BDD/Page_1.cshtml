@{
                /**/

                var postits = (List<List<string>>)ViewData["UserStorys"];
}

<div class="content-header">
    <h1>
        User Storys projeto escolhido.
    </h1>
    <div class="breadcrumb">
        @Ajax.ActionLink("Voltar", "Index", "BDD", null, new AjaxOptions { InsertionMode = InsertionMode.Replace, UpdateTargetId = "Ajax_Update_Menu" }, new { @class = "btn btn-warning" })
        @Ajax.ActionLink("Continuar", "Page_2", "BDD", null, new AjaxOptions { InsertionMode = InsertionMode.Replace, UpdateTargetId = "Ajax_Update_Menu" }, new { @class = "btn btn-primary", @id = "btnContinuar" })
    </div>
</div>
<div class="content">
    <div class="box">
        <div class="box-header with-border">
            <div class="col-lg-12">
                <div>
                    <h4>A lousa abaixo, contem os Projetos referente ao seu projeto.</h4>
                    <p>Escolha um deles para começarmos o tutorial do BDD.</p>
                </div>
            </div>
        </div>
        <div class="box-body">
            <div class="board">
                <div class="row">
                    @foreach (var lines in postits)
                    {
                        <div class="col-sm-12 col-md-4 col-lg-2">
                            <div class="alert postit select">
                                @foreach (var line in lines)
                                {
                                    <div class="p">
                                        <p>@line</p>
                                    </div>
                                }
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</div>

@Helpers.PostitExampleHTML(classCol: "col-sm-12 col-md-4", addClassPostit: "select")

@section scripts {
    <script type="text/javascript">
        $('.postit').click(function () {
            $('.postit').removeClass('active');
            $(this).addClass('active');
        });
    </script>
}