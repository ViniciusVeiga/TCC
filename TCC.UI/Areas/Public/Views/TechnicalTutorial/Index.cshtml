@model TCC.Domain.Entities.Admin.ETTechnicalTutorial

@{ 
    var urlContent = "/Content/Index";
}

<div class="content-header">
    <h1>
        @Model.Title
    </h1>
    <ul class="breadcrumb">
        @Ajax.ActionLink("Voltar", "Index", "Content/Index", null, new AjaxOptions { InsertionMode = InsertionMode.Replace, UpdateTargetId = "Ajax_Update_Menu" }, new { @class = "btn btn-warning" })
    </ul>
</div>
<div class="content">
    <div class="box">
        <div class="box-body">
            <div class="col-lg-12">
                @Html.Raw(Model.Text)
            </div>
        </div>
    </div>
</div>

@section scripts {
    @Scripts.Render("~/Scripts/Prism")
}