@using TCC.UI.Helpers.Extensions
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Test Tutorials</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="shortcut icon" href="/favicon.ico" />
    <link rel=stylesheet href="~/Content/CodeMirror/lib/codemirror.css">

    @Styles.Render("~/Styles/Components")
    @Styles.Render("~/Styles/AdminLTE")
    @Styles.Render("~/Styles/DataTable")
    @Styles.Render("~/Styles/Site")
    @Styles.Render("~/Styles/iCheck")
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        @Helpers.LoadingPage()

        @Html.Partial("_Header")
        @Html.Partial("_Sidebar")

        <div id="Ajax_Update_Menu" class="content-wrapper">
            @RenderBody()
        </div>

        <div class="main-footer">
        </div>

        @Html.Partial("Login/_Login")
        @Html.Partial("Login/_NewAccount")
        @Html.Partial("Login/_Logoff")
    </div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.40/pdfmake.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.40/vfs_fonts.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.18.1/moment.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/pdfjs-dist@2.0.385/build/pdf.min.js"></script>

    @Helpers.LoadingInitial()

    @Scripts.Render("~/Scripts/Base")
    @Scripts.Render("~/Scripts/AdminLTE")
    @Scripts.Render("~/Scripts/Validate")
    @Scripts.Render("~/Scripts/Site")

    @Html.Partial("Login/Scripts")
    @Html.Partial("Toastr/_Toestr")

    @this.WriteScriptBlocks()
    @RenderSection("scripts", required: false)
</body>
</html>
