@model TCC.Domain.Entities.Admin.ETContent

@{
    ViewBag.Title = Model.Name;
}

<div class="content-header">
    <h1>
        @ViewBag.Title
    </h1>
</div>
<div class="content">

    <div class="box">
        <div class="box-header">
            <h3 class="box-title">Sobre @ViewBag.Title</h3>
        </div>
        <div class="box-body">
            <div class="col-lg-12">
                @Html.Raw(Model.Text)
            </div>
        </div>
    </div>
</div>

<script src="~/Content/Prism/prism.js"></script>