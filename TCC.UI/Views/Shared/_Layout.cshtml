<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Test Tutorials</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

    @Styles.Render("~/Css/Components")
    @Styles.Render("~/Css/AdminLTE")
    @Styles.Render("~/Css/Site")
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        @Html.Partial("_Header")
        @Html.Partial("_Sidebar")

        <div class="content-wrapper">
            @RenderBody()
        </div>
        <div class="main-footer">
        </div>
        @Html.Partial("Login/_Login")
        @Html.Partial("Login/_NewAccount")
        @Html.Partial("Login/_Logoff")
    </div>

    @Scripts.Render("~/Js/jquery")
    @Scripts.Render("~/Js/bootstrap")
    @Scripts.Render("~/Js/AdminLTE")
    @Scripts.Render("~/Js/site")
    @Scripts.Render("~/Js/toastr")

    @Html.Partial("Login/Scripts")
    @Html.Partial("Toastr/_Toestr")

    @RenderSection("scripts", required: false)
</body>
</html>
